{
  "name": "pp-gravel",
  "version": "0.1.8",
  "private": false,
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "antd": "3.24.0",
    "babel-plugin-import": "^1.12.2",
    "customize-cra": "^0.8.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "react": "^16.10.2",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "BABEL_ENV=production babel src -d dist --extensions '.tsx,.ts'",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "babel": {
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ],
    "presets": [
      [
        "react-app",
        {
          "flow": false,
          "typescript": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-docs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "auto": "^7.8.0",
    "prettier": "^1.18.2",
    "storybook-addon-designs": "^5.1.1"
  },
  "main": "dist/index",
  "typings": "dist/index",
  "repository": "https://github.com/ZhenanH/gravel.git",
  "author": "Zhenan Hong <zhong@pebblepost.com>",
  "license": "MIT"
}
